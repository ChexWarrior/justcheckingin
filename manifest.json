{
  "manifest_version": 2,
  "name": "Just Checking In",
  "version": "0.1",
  "description": "A weird browser extension I made for my girlfriend",
  "homepage_url": "https://github.com/ChexWarrior/justcheckingin",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["main.css"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
    "*://www.random.org/*"
  ],
  "web_accessible_resources" : [
    "portraits/*"
  ]
}
