{
  "manifest_version": 2,
  "name": "Just Checking In",
  "version": "0.1",
  "description": "A weird browser extension I made for my girlfriend",
  "homepage_url": "https://github.com/ChexWarrior/justcheckingin",
  "background" : {
    "scripts": ["background.js"]
  },
  "permissions": [
    "*://www.random.org/*",
    "*://fortunecookieapi.herokuapp.com/*",
    "notifications",
    "alarms"
  ],
  "web_accessible_resources" : [
    "portraits/*"
  ]
}
